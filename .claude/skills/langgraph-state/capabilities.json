{
  "$schema": "../../schemas/skill-capabilities.schema.json",
  "name": "langgraph-state",
  "version": "1.0.0",
  "description": "LangGraph state management patterns for agent workflows",
  "capabilities": [
    "state-channels",
    "state-definition",
    "state-persistence",
    "state-reducers",
    "subgraphs"
  ],
  "triggers": {
    "high_confidence": [
      "LangGraph.*state",
      "StateGraph",
      "TypedDict.*state"
    ],
    "medium_confidence": [
      "state channel",
      "reducer",
      "workflow state",
      "Annotated"
    ]
  },
  "integrates_with": [
    "langgraph-checkpoints",
    "langgraph-routing"
  ]
}
