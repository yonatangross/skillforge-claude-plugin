{
  "$schema": "../../schemas/skill-capabilities.schema.json",
  "name": "cache-cost-tracking",
  "version": "1.0.0",
  "description": "LLM caching strategies and cost tracking patterns",
  "capabilities": [
    "cache-invalidation",
    "cost-calculation",
    "prompt-caching",
    "response-caching",
    "usage-tracking"
  ],
  "triggers": {
    "high_confidence": [
      "cache.*cost",
      "cost.*tracking",
      "token.*usage"
    ],
    "medium_confidence": [
      "cache",
      "cost tracking",
      "LLM cost",
      "cache hit"
    ]
  },
  "integrates_with": [
    "semantic-caching",
    "langfuse-observability"
  ]
}
