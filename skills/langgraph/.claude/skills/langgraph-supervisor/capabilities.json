{
  "$schema": "../../../../../.claude/schemas/skill-capabilities.schema.json",
  "name": "langgraph-supervisor",
  "version": "1.0.0",
  "description": "Supervisor-worker pattern for LangGraph multi-agent orchestration",
  "capabilities": [
    "content-analysis",
    "skillforge-workflow",
    "supervisor-design",
    "supervisor-template",
    "worker-delegation"
  ],
  "triggers": {
    "high_confidence": [
      "supervisor.*worker",
      "orchestrat.*agent",
      "langgraph.*supervisor"
    ],
    "medium_confidence": [
      "multi-agent.*route",
      "delegate.*worker"
    ]
  },
  "integrates_with": [
    "langgraph-state",
    "langgraph-routing",
    "langgraph-checkpoints"
  ]
}
