{
  "$schema": "../../../.claude/schemas/skill-capabilities.schema.json",
  "name": "semantic-caching",
  "version": "1.0.0",
  "description": "Redis-based semantic similarity caching for LLM responses",
  "capabilities": [
    "cache-service",
    "redis-vector-cache",
    "similarity-threshold",
    "skillforge-integration"
  ],
  "triggers": {
    "high_confidence": [
      "semantic.*cache",
      "redis.*vector",
      "llm.*cache"
    ],
    "medium_confidence": [
      "cache.*similarity",
      "embedding.*cache"
    ]
  },
  "integrates_with": [
    "prompt-caching",
    "cache-cost-tracking",
    "embeddings"
  ]
}
