            "command": "${CLAUDE_PLUGIN_ROOT}/hooks/pretool/write-edit/multi-instance-lock.sh"
          },
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/hooks/pretool/write-edit/file-lock-check.sh"
          }
        ]
      },
      {
        "matcher": "Skill",
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/hooks/pretool/skill/skill-tracker.sh"
          }
        ]
      },
      {
        "matcher": "mcp__context7__*",
        "hooks": [
